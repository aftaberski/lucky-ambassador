<%= @question.title %><br>
<% @question.commentable.each do |comment| %>
  <%= comment.content %>
  <%= render partial: "form"%>
<% end %>
<% @question.answers.each do |answer| %>
  <%= answer.content %><br>
  HI
  <% answer.comments.each do |comment| %>
    <%= comment.content %>
  <% end %>
  <%= render partial: "answers/form"%>
<% end %>